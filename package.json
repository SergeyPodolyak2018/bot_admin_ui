    "@stripe/stripe-js": "^2.4.0",
    "@types/howler": "^2.2.11",
    "@types/recordrtc": "^5.6.14",
    "@yaireo/tagify": "4.24.0",
    "ag-charts-community": "^9.3.1",
    "ag-charts-react": "^9.3.1",
    "ag-grid-react": "^31.1.1",
    "axios": "^1.6.7",
