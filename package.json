{
  "name": "chatbot-admin-ui",
  "workspaces": [
    "widget_chat",
    "widget_call"
  ],
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsc && vite --mode development",
    "build": "tsc && vite build --mode production && npm run chat-widget && npm run call-widget",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "tscheck": "tsc --project tsconfig.json",
    "preview": "vite preview",
    "prettier-watch": "onchange \"**/*.tsx\" -- prettier --write {{changed}}",
    "prepare": "husky install",
    "lint:prod": "eslint --max-warnings=0 --report-unused-disable-directives --ext .ts,tsx src",
    "prettier:fix": "prettier --write ./src",
    "chat-widget": "vite build --config ./widget_chat/vite.config.ts",
    "call-widget": "vite build --config ./widget_call/vite.config.ts"
  },
  "dependencies": {
